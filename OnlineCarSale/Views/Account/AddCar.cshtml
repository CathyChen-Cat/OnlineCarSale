
@{
    ViewBag.Title = "AddCar";
}

<form class="row mt-3 mb-3">
    <div class="col-12 mt-3">
        <h3>Please Fill In Car Detail</h3>
    </div>
    <div class="col-12 mt-3">
        <label for="inputMake" class="form-label">Car Make Company</label>
        <input type="text" class="form-control" id="inputMake">
    </div>
    <div class="col-12 mt-3">
        <label for="inputCarModel" class="form-label">Car Model</label>
        <input type="text" class="form-control" id="inputCarModel">
    </div>
    <div class="col-12 mt-3">
        <label for="inputCarYear" class="form-label">Car Year</label>
        <input type="text" class="form-control" id="inputCarYear">
    </div>
    <div class="col-12 mt-3">
        <label for="inputBodyType" class="form-label">Body Type</label>
        <input type="text" class="form-control" id="inputBodyType">
    </div>
    <div class="col-12 mt-3">
        <label for="inputPrice" class="form-label">Price</label>
        <input type="text" class="form-control">
    </div>
    <div class="col-12 mt-3">
        <label for="inputLocation" class="form-label">Location</label>
        <input type="text" class="form-control" id="inputLocation">
    </div>
    <div class="col-12 mt-3">
        <button type="submit" class="btn btn-primary">Add</button>
    </div>
</form>

