@model OnlineCarSale.Models.Seller

@{
    ViewBag.Title = "SellerDetail";
}

<h3 class="mt-3 mb-3">Your Details</h3>

@if (Session["Name"] != null)
{
    <table class="table-primary mb-3">
        <tbody>
            <tr>
                <th scope="row"> Name </th>
                <td>@Html.DisplayFor(s => s.Name)</td>
            </tr>
            <tr>
                <th scope="row"> Address </th>
                <td>@Html.DisplayFor(s => s.Address)</td>
            </tr>
            <tr>
                <th scope="row"> Phone </th>
                <td>@Html.DisplayFor(s => s.Phone)</td>
            </tr>
            <tr>
                <th scope="row"> Email </th>
                <td>@Html.DisplayFor(s => s.Email)</td>
            </tr>
            <tr>
                <th scope="row"> Username </th>
                <td>@Html.DisplayFor(s => s.Username)</td>
            </tr>
            <tr>
                <th scope="row"> Passowrd </th>
                <td>@Html.DisplayFor(s => s.Passowrd)</td>
            </tr>
            <tr>
                <td>
                    <a href="@Url.Action("Logout","Login")" class="btn btn-warning">Logout</a>
                </td>
            </tr>
        </tbody>
    </table>
}

<div class="row mt-3 mb-3">
    <button type="button" class="btn btn-primary" onclick="location.href = '@Url.Action("Addcar", "Account")'">
        <i class="bi bi-plus-circle"> Add New Car </i>
    </button>
</div>


